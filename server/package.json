{
  "name": "portfolio-two-server",
  "version": "0.0.0",
  "scripts": {
    "start": "export $(cat .env|xargs) && nodemon --harmony_generators server.js",
    "empty-db": "export $(cat .env|xargs) && node --harmony_generators server.js empty",
    "sync-db": "export $(cat .env|xargs) && node --harmony_generators server.js design-docs,populate"
  },
  "devDependencies": {
    "nodemon": "~1.0.15"
  },
  "dependencies": {
    "koa-route": "~1.1.0",
    "koa": "~0.5.1",
    "koa-static": "~1.4.3",
    "koa-send": "~1.2.3",
    "nano": "~5.7.1",
    "co-nano": "0.0.3",
    "co": "~3.0.5",
    "couchdb-tools": "~0.0.1",
    "require-all": "0.0.8"
  }
}
